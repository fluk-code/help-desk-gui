{
  "name": "help-desk-gui",
  "version": "0.0.0",
  "description": "### ⚠️ Check List",
  "main": "index.js",
  "author": "Felipe Ferreira",
  "license": "UNLICENSED",
  "scripts": {
    "ng": "npx ng",
    "start": "npx ng serve --host 0.0.0.0",
    "build": "npx ng build",
    "watch": "npx ng build --watch --configuration development",
    "test": "npx ng test --browsers ChromeHeadless",
    "test:ci": "npx ng test --watch=false --reporters junit --browsers ChromeHeadless",
    "test:cov": "npx ng test --watch=false --code-coverage --browsers ChromeHeadless",
    "test:staged": "npm run test -- --findRelatedTests",
    "lint": "eslint ./src --ext .ts",
    "lint:fix": "npm run lint --fix",
    "formatter:check": "prettier --check 'src/**/*.ts'",
    "formatter:fix": "prettier --write 'src/**/*.ts'",
    "serve:ssr:help-desk-gui": "node dist/help-desk-gui/server/server.mjs"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^17.0.0",
    "@angular/common": "^17.0.0",
    "@angular/compiler": "^17.0.0",
    "@angular/core": "^17.0.0",
    "@angular/forms": "^17.0.0",
    "@angular/platform-browser": "^17.0.0",
    "@angular/platform-browser-dynamic": "^17.0.0",
    "@angular/platform-server": "^17.0.0",
    "@angular/router": "^17.0.0",
    "@angular/ssr": "^17.0.6",
    "@types/socket.io-client": "^1.4.36",
    "express": "^4.18.2",
    "rxjs": "~7.8.0",
    "socket.io-client": "^4.8.1",
    "tslib": "^2.3.0",
    "zone.js": "~0.14.2"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^17.0.6",
    "@angular/cli": "^17.0.6",
    "@angular/compiler-cli": "^17.0.0",
    "@commitlint/cli": "^18.4.3",
    "@types/express": "^4.17.17",
    "@types/jasmine": "~5.1.0",
    "@types/node": "^18.18.0",
    "@typescript-eslint/eslint-plugin": "^6.13.2",
    "@typescript-eslint/parser": "^6.13.2",
    "commitlint-config-jira": "^1.6.4",
    "commitlint-plugin-jira-rules": "^1.6.4",
    "eslint": "^8.55.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-import-resolver-typescript": "^3.6.1",
    "eslint-plugin-import-helpers": "^1.3.1",
    "eslint-plugin-prettier": "^5.0.1",
    "eslint-plugin-sonarjs": "^0.23.0",
    "husky": "^8.0.3",
    "jasmine-core": "~5.1.0",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.2.0",
    "karma-coverage": "~2.2.0",
    "karma-firefox-launcher": "^2.1.2",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.1.0",
    "karma-junit-reporter": "^2.0.1",
    "karma-mocha-reporter": "^2.2.5",
    "lint-staged": "^15.2.0",
    "prettier": "^3.1.0",
    "sonarlint": "^1.1.0",
    "typescript": "~5.2.2"
  }
}
